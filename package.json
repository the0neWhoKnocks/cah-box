{
  "name": "cah-box",
  "description": "Cards Against Humanity mixed with the play-on-any-device strategy of JackBox",
  "version": "0.0.1",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "NODE_ENV=production sapper build",
    "dev": "sapper dev --no-hot",
    "export": "sapper export",
    "preinstall": "./bin/pre-install.sh",
    "start": "node __sapper__/build",
    "test": "npm run build && concurrently --kill-others -p \"[ {name} ]\" -n SERVER,TESTS -c black.bgGreen,black.bgCyan \"npm run start\" \"npx cypress open\""
  },
  "dependencies": {
    "compression": "^1.7.1",
    "eslint": "^7.5.0",
    "polka": "next",
    "sirv": "^0.4.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cypress-browser-permissions": "^1.0.5",
    "eslint-plugin-cypress": "^2.11.1",
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  }
}
