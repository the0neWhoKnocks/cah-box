{
  "name": "cah-box",
  "description": "Cards Against Humanity mixed with the play-on-any-device strategy of JackBox",
  "version": "0.0.2",
  "engines": {
    "node": "10.22.x"
  },
  "scripts": {
    "build": "./bin/prep-dist.sh && NODE_ENV=production webpack",
