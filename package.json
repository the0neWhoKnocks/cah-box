    "browser-sync": "^2.26.12",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.5.0",
    "eslint-plugin-cypress": "^2.11.1",
    "ignore-emit-webpack-plugin": "^2.0.3",
    "mini-css-extract-plugin": "^0.12.0",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.1.0",
    "svelte": "^3.29.0",
