{
  "version": "0.0.2",
  "scripts": {
    "build": "./bin/prep-dist.sh && NODE_ENV=production webpack",
    "lint": "eslint",
    "preinstall": "if [ -z \"$IN_CONTAINER\" ] || ! $IN_CONTAINER; then echo \" [ERROR] Not in Docker\\n\"; rm -rf node_modules; exit 1; fi",
    "start": "node ./dist/server",
    "start:dev": "./bin/prep-dist.sh && webpack & ./watcher.js \"./bin/prep-dist.sh\" \"./dist/public/manifest.json\"",
    "test": "./e2e/bin/test-runner.sh --name cahbox",
    "test:watch": "npm run test -- --watch"
  },
  "engines": {
    "node": ">=24.1.0"
  },
  "dependencies": {
    "anylogger": "1.0.11",
    "compression": "1.8.1",
    "polka": "1.0.0-next.23",
    "sirv": "3.0.1",
    "ulog": "2.0.0-beta.19",
    "ws": "8.18.3"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.2",
    "@stylistic/eslint-plugin": "5.2.3",
    "browser-sync": "3.0.4",
    "chokidar": "3.5.2",
    "clean-webpack-plugin": "4.0.0",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.2",
    "eslint": "9.33.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-playwright": "1.7.0",
    "eslint-plugin-svelte": "3.11.0",
    "globals": "16.3.0",
    "ignore-emit-webpack-plugin": "2.0.6",
    "mini-css-extract-plugin": "2.9.4",
    "nodemon": "3.0.1",
    "svelte": "5.38.2",
    "svelte-loader": "3.2.4",
    "svelte-portal": "2.2.0",
    "terser-webpack-plugin": "5.2.5",
    "webpack": "5.101.3",
    "webpack-cli": "4.9.1",
    "webpack-manifest-plugin": "4.0.2"
  }
